{
    "problemMatcher": [
        {
            "owner": "stylelint-compact",
            "pattern": [
                {
                    "regexp": "^(.+): line (\\d+), col (\\d+), (error|warning|info) - (.+) \\((.+)\\)",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5,
                    "code": 6
                }
            ]
        }
    ]
}
