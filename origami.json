		},
		{
			"title": "Headings",
			"name": "headings",
			"template": "demos/src/headings.mustache"
		},
		{
			"title": "Body",
			"name": "body",
			"template": "demos/src/body.mustache"
		},
		{
			"title": "Unordered List",
			"name": "unordered-list",
			"documentClasses": "o-editorial-typography-body",
			"template": "demos/src/unordered-list.mustache"
		},
		{
			"title": "Ordered List",
			"name": "ordered-list",
			"documentClasses": "o-editorial-typography-body",
			"template": "demos/src/ordered-list.mustache"
		},
		{
			"title": "Standfirst",
			"name": "standfirst",
			"template": "demos/src/standfirst.mustache"
		},
		{
			"title": "Byline With Timestamp",
			"name": "byline",
			"template": "demos/src/byline.mustache"
