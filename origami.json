		},
		{
			"title": "Caption",
			"name": "caption",
			"template": "demos/src/caption.mustache"
		},
		{
			"title": "Pa11y",
			"name": "pa11y",
			"documentClasses": "o-editorial-typography-body",
			"template": "demos/src/pa11y.mustache",
			"description": "Accessibility test will be run against this demo",
			"hidden": true
		}
	]
