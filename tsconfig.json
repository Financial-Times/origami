{
    "compilerOptions": {
		"jsx": "react-jsx",
		"module": "esnext",
		"target": "es2017",
		"lib": ["es6", "dom", "es2016", "es2017"],
		"declaration": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"esModuleInterop": true,
		"skipLibCheck": true
    },
}
