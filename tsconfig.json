{
	"compilerOptions": {
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"target": "ES2019",
		"moduleResolution": "node",
		"sourceMap": true,
		"declaration": true,
		"declarationMap": true
	},
	"exclude": ["node_modules"]
}
