---
import DocLayout from "@/layouts/DocLayout.astro"
import MainContent from "@/components/docLayout/MainContent.astro"

const title = "Origami Manifests"
const description =
	"Origami manifest files are used to describe and configure Origami projects."
const collectionName = "documentation"
const collectionFilterCallback = ({id}) => id.startsWith("manifests/")
---

<DocLayout
	title={title}
	description={description}
	secondaryNav={{
		renderSecondaryNav: true,
		collectionName,
		collectionFilterCallback,
	}}
>
	<MainContent
		title={title}
		description={description}
		collectionName={collectionName}
		collectionFilterCallback={collectionFilterCallback}
	/>
</DocLayout>
