{
  "name": "origami-storybook",
  "version": "1.0.0",
  "description": "origami's storybook",
  "scripts": {
    "storybook": "storybook dev -p ${PORT:-6969}",
    "build-sb:core": "ORIGAMI_STORYBOOK_BRAND=core storybook build -o ../../origami.ft.com/storybook/brands/core",
    "build-sb:internal": "ORIGAMI_STORYBOOK_BRAND=internal storybook build -o ../../origami.ft.com/storybook/brands/internal",
    "build-sb:whitelabel": "ORIGAMI_STORYBOOK_BRAND=whitelabel storybook build -o ../../origami.ft.com/storybook/brands/whitelabel",
    "build-storybook": "run-p build-sb:*"
  },
  "dependencies": {
    "@storybook/addon-a11y": "^7.4.5",
    "@storybook/addon-actions": "^7.4.5",
    "@storybook/addon-designs": "^7.0.5",
    "@storybook/addon-links": "^7.4.5",
    "@storybook/addons": "^7.4.5",
    "@storybook/builder-webpack5": "^7.4.5",
    "@storybook/cli": "^7.4.5",
    "@storybook/components": "^7.4.5",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^7.4.5",
    "@storybook/react-docgen-typescript-plugin": "^1.0.2-canary.f7e28c0.0",
    "@storybook/react-webpack5": "^7.4.5",
    "@storybook/theming": "^7.4.5",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "nanoid": "^3.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-notion": "^0.10.0",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "storybook": "^7.4.5",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.2"
  },
  "author": "chee <chee@snoot.club>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0"
  }
