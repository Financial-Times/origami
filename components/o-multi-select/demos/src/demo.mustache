<div
	class="o-multi-select"
	data-o-component="o-multi-select"
>
	<div class="o-multi-select_core">
		<label for="options" id="combo-label" className="combo-label">
			Multi-select Combobox Example core
		</label>
		<select name="options" id="multiSelect" multiple>
			{{#multiSelectOptions}}
			<option value="{{.}}">{{.}}</option>
			{{/multiSelectOptions}}
		</select>
	</div>
	<div class="o-multi-select_enhanced">
		<label id="combo-label" className="combo-label">
			Multi-select Combobox Example enhanced
		</label>
		<span id="combo-remove" style="display: none">remove</span>
		<ul className="selected-options" id="combo-selected"></ul>
		<div className="combo js-multiselect">
			<input
				aria-activedescendant=""
				aria-autocomplete="none"
				aria-controls="listbox3"
				aria-expanded="false"
				aria-haspopup="listbox"
				aria-labelledby="combo-label combo-selected"
				id="combo"
				className="combo-input"
				role="combobox"
				type="text"
			/>
			<div
				className="combo-menu"
				role="listbox"
				id="listbox3"
				aria-label="multi select options"
			></div>
		</div>
	</div>
</div>
